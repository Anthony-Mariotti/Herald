{
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console"
        ],
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
            "Microsoft": "Warning",
            "DSharpPlus": "Information"
            }
        },
        "WriteTo": [
            {
                "Name": "Console"
            }
        ],
        "Enrich": [
            "FromLogContext"
        ]
    },
    "HeraldConfig": {
        "DiscordKey": "",
        "Lavalink": {
            "RestUri": "http://host.docker.internal:2333/",
            "WebSocketUri": "ws://host.docker.internal:2333/",
            "Password": "youshallnotpass",
            "DisconnectOnStop": true,
            "AllowResuming": true,
            "UserAgent": "Herald/1.0.0-alpha",
            "Label": "local"
        },
        "AnyDealApiKey": ""
    },
    "Database": {
        "Server": "localhost",
        "Port": 3306,
        "Name": "Herald",
        "Username": "root",
        "Password": "my-secret-pw",
        "ConnectionTimeout": 5,
        "CommandTimeout": 10,
        "Feature": {
            "UsageAdvisor": false,
            "PerformanceMonitor": false
        }
    }
}
